<h2>School Class Form</h2>

<%= form_for(@school_class) do |f| %>
  <label for="school_class_title">School class title:</label>
  <%= f.text_field :title %><br>
  <label for="school_class_room_number">School class room_number:</label>
  <%= f.text_field :room_number %><br>
  <%= f.submit "Update School class"%>
<% end %>

